<html t:type="AppLayout" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd">
	
	<t:form t:id="telephoneform">
		<div class="panel panel-default">			
			<div class="panel panel-primary">
				<div class="panel-heading">
					<t:label for="newTelephone">Añadir teléfono</t:label>
				</div>
			</div>

			<div class="panel-body">
				<div class="container-fluid">
					<div class="row">
						<div class="col-md-6">
							<div class="row">
								<t:textfield t:id="newTelephone" placeholder="Nuevo teléfono"/>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</t:form>

</html>
